    author = {Nigel CS Tan and Ivan Djordjevic and Jamie Ann Malley and Ailyn LQ Kwang and Syed Ikhwan and Ivan Šolić and Juhi Singh and Gautama Wicaksono and Sierin Lim and Terry WJ Steele},
